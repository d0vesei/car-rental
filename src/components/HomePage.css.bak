.home-container {
    text-align: center;
    margin-top: 60px;
  }
  
  .App-header, .home-container {
    margin-bottom: 0; 
    padding-bottom: 0;
  }

  .navbar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px;
    background-color: #f0f0f0;
    position: fixed; 
    top: 0; 
    width: 100%; 
    z-index: 1000; 
  }
  
  .navbar-brand {
    font-size: 1.5em;
    font-weight: bold;
    visibility: hidden;
  }
  
  .navbar-links .nav-link {
    margin-right: 15px;
    padding: 10px 15px;
    text-decoration: none;
    color: black;
  }
  
  .nav-link.active {
    font-weight: bold;
  }

  .nav-link:hover {
    background-color: #555; 
    color: black; 
  }
  
  .header {
    background-color: #e0e0e0;
    padding: 20px;
  }
  
  .btn-main {
    background-color: blue;
    color: white;
    padding: 10px 15px;
    border: none;
    border-radius: 5px;
    cursor: pointer;
  }
  
  
  .about-us {
    margin-top: 20px;
  }
  
  .about-us h2 {
    color: black; 
  }
  
  .about-us p {
    color: black; 
    text-align: left; 
  }